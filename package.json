{
  "name": "imust",
  "version": "0.0.1-beta.3",
  "description": "a command line tool for developers",
  "main": "./lib/index.js",
  "bin": {
    "imust": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts hello",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "npm run build && sudo npm i -g && imust",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test": "echo \"No tests specidied - procceed\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connormaglynn/developer-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connormaglynn/developer-cli/issues"
  },
  "homepage": "https://github.com/connormaglynn/developer-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.3.0",
    "figlet": "^1.5.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/node": "^18.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
